<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="356.0" prefWidth="341.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.Controllers.scenes_controllers">
    <children>
        <Label layoutX="20" layoutY="20" text="Seleccione el curso a eliminar" />
        <ListView fx:id="courseListView" layoutX="20" layoutY="50" prefHeight="200" prefWidth="300" />
        <Button layoutX="20" layoutY="270" onAction="#handleDeleteCourse" text="Eliminar Curso" />
        <Label fx:id="messageLabel" layoutX="20" layoutY="310" textFill="red" />
      <Button layoutX="270.0" layoutY="270.0" onAction="#switchToCoursesMenu" text="Volver" />
    </children>
</AnchorPane>
